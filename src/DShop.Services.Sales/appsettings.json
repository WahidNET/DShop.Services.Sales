{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "application": {
    "name": "DShop Sales Service",
    "seedData": false
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "sales-service",
    "address": "docker.for.win.localhost",
    "port": "5008",
    "pingEnabled": false,
    "pingEndpoint": "",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "sales-service",
    "requestRetries": 3
  },
  "rabbitMq": {
    "namespace": "sales",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "orders-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5005"
      }
    ]
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "sql": {
    "connectionString": "Server=localhost;User ID=SA; Password=Abcd1234!;Database=DShopSales",
    "database": "DShopSales",
    "inMemory": true
  },
  "vault": {
    "enabled": true,
    "url": "http://localhost:8200",
    "key": "sales-service/settings",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret"
  }
}
